<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<TitledPane alignment="CENTER" animated="false" collapsible="false" text="Team info" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <BorderPane>
            <center>
                <SplitPane dividerPositions="0.7" orientation="VERTICAL" prefHeight="700.0" prefWidth="600.0" styleClass="sub_background">
                    <items>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="598.0" styleClass="sub_background">
                            <children>
                                <Text layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Team name" />
                                <Text fx:id="teamName" layoutX="124.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" />
                                <Text layoutX="14.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Team contact" />
                                <Text fx:id="teamContact" layoutX="124.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" />
                                <Text layoutX="14.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Team members" />
                                <Text fx:id="teamMembers" layoutX="124.0" layoutY="91.0" strokeType="OUTSIDE" strokeWidth="0.0" />
                                <ImageView fx:id="teamLogo" fitHeight="156.0" fitWidth="182.0" layoutX="399.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true" />
                            </children>
                        </AnchorPane>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" styleClass="sub_background" />
                    </items>
                </SplitPane>
            </center>
        </BorderPane>
    </content>
</TitledPane>
